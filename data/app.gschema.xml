<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="victrola">
  <schema id="@app_id@" path="/com/fyralabs/Victrola/">
    <key name="music-dir" type="s">
      <default>''</default>
      <summary>Load music from folder</summary>
    </key>
    <key name="played-uri" type="s">
      <default>''</default>
      <summary>Played uri when quit</summary>
    </key>
    <key name="sort-mode" type="u">
      <range min="0" max="5"/>
      <default>5</default>
      <summary>Sort Mode</summary>
    </key>
  </schema>
</schemalist>
